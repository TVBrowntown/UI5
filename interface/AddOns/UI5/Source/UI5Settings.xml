				</Anchors>
			</Button>

			<Button name="UI5TalentsButton" inherits="DefaultButton" backgroundtexture="UF_AB_Talents" layer="default" texturescale="1">
				<Size>
					<AbsPoint x="34" y="24"/>
				</Size>
